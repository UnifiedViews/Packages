#!/bin/sh
#

set -e

. /usr/share/debconf/confmodule


case "$1" in
  remove|upgrade|deconfigure)
  service unifiedviews-backend stop
    
    ;;

  failed-upgrade)
  ;;

  *)
    echo "prerm called with unknown argument \`$1'" >&2
    exit 1
  ;;
esac

# generated by other debhelper scripts.

#DEBHELPER#

exit 0


